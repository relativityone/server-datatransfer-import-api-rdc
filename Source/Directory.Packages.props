    <PackageVersion Include="Castle.Windsor" Version="3.3.0" />
    <PackageVersion Include="ConfigureAwaitChecker.Analyzer" Version="4.0.0" />
    <PackageVersion Include="CSVHelper" Version="12.1.2" />

    <!-- This is used strictly for producing unit test results. -->
    <PackageVersion Include="AutoBogus" Version="2.13.0" />
    <PackageVersion Include="Bogus" Version="31.0.3" />
    <PackageVersion Include="Extent" Version="0.0.3" />
    <PackageVersion Include="FileHelpers" Version="3.4.0" />
    <PackageVersion Include="FluentAssertions" Version="5.10.3" />
    <PackageVersion Include="ILMerge" Version="3.0.29" />
