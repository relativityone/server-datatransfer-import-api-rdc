    <PackageVersion Include="Relativity.Audit.Services.SDK" Version="17.2.3" />
    <PackageVersion Include="Relativity.AutomatedWorkflows.Services.Interfaces" Version="0.1.1-pre-4" />
    <PackageVersion Include="Relativity.DataExchange.TestData" Version="1.0.0" />
    <PackageVersion Include="Relativity.Logging" Version="2019.5.1" />
    <PackageVersion Include="Relativity.Logging.Interfaces" Version="2019.5.1" />
    <PackageVersion Include="Relativity.Kepler" Version="2.15.0" />
