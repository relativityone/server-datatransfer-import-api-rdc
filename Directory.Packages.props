		<!-- group ui-automation -->
		<PackageVersion Include="Appium.WebDriver" Version="3.0.0.2" />
		<PackageVersion Include="selenium.support" Version="3.8.0" />
		<PackageVersion Include="selenium.webdriver" Version="3.8.0" />
		<PackageVersion Include="Relativity.DataExchange.TestData" Version="1.0.0" />

		<!-- group integration-tests -->
		<PackageVersion Include="System.ValueTuple" Version="4.5.0" />
		
		<!-- Relativity.DataExchange.NUnit.Integration -->	
		<PackageVersion Include="relativity.telemetry.datacontracts" Version="3003.1.0" />
