		<PackageVersion Include="kcura.relativity.client" Version="11.2.8.6-DEV" />
		<PackageVersion Include="relativity.logging" Version="2019.5.1" />
		<PackageVersion Include="Relativity.Audit.Services.SDK" />
		<PackageVersion Include="relativity.productions.services.interfaces" Version="11.0.7" />
		<PackageVersion Include="relativity.testing.identification" Version="3.6.0" />
		<PackageVersion Include="ReportUnit" Version="1.2.1" />

