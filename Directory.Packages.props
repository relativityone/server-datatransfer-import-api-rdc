		<PackageVersion Include="polly" Version="5.7.0" />
		<PackageVersion Include="psake" Version="4.7.4" />
		<PackageVersion Include="Relativity.AutomatedWorkflows.Services.Interfaces" Version="0.1.1-pre-4 pre" />
		<PackageVersion Include="reportgenerator" Version="4.1.2" />

