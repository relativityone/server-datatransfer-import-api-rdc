<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<PropertyGroup>
		<ManagePackageVersionsCentrally>true</ManagePackageVersionsCentrally>
		<CentralPackageTransitivePinningEnabled>false</CentralPackageTransitivePinningEnabled>
	</PropertyGroup>
	<ItemGroup>
		
		<PackageVersion Include="AutoMapper" Version="10.1.1" />
		<PackageVersion Include="AutoFixture" Version="4.17.0" />
		<PackageVersion Include="Fare" Version="2.1.1" />

		<!-- Note: do NOT change the Castle version due to SOI with RIP and potentially other consumers. -->
		<PackageVersion Include="castle.core" Version="3.3.3" />
		<PackageVersion Include="castle.windsor" Version="3.3.0" />
		<PackageVersion Include="ConfigureAwaitChecker.Analyzer" />
		<PackageVersion Include="csvhelper" Version="12.1.2" />

		<!-- This is used strictly for producing unit test results. -->
		<PackageVersion Include="extent" Version="0.0.3" />
		<PackageVersion Include="filehelpers" Version="3.4.0" />
		<PackageVersion Include="ilmerge" Version="3.0.29" />
		<PackageVersion Include="jetbrains.dotcover.commandlinetools" Version="2019.1.0-eap06" />
		<PackageVersion Include="microsoft.build.logging.structuredlogger" Version="1.0.89" />
