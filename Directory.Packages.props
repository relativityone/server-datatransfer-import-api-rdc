		<PackageVersion Include="serilog" Version="2.6.0" />
		<PackageVersion Include="rhinomocks " Version="3.6.1" />
		<PackageVersion Include="stylecop.analyzers" Version="1.1.118" />

		<!-- Be careful with this library because there are several similarly named packages. -->
		<!-- This represents the most current and actively developed version. -->
		<PackageVersion Include="microsoft-windowsapicodepack-core" Version="1.1.3.3" />

		<!-- Note: do NOT change the Reactive version due to SOI. -->
		<PackageVersion Include="System.Reactive" Version="4.0.0" />

