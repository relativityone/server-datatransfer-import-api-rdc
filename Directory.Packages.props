		<PackageVersion Include="EWSoftware.SHFB" Version="2019.8.24" />

		<!-- The Relativity packages below are required by the SDK projects. -->
		<PackageVersion Include="Relativity.OutsideIn" Version="2021.8.0" />
		<PackageVersion Include="relativity.logging.interfaces" Version="2019.5.1" />
		<PackageVersion Include="Relativity.ObjectManager" Version="12.1.171.20" />
		<PackageVersion Include="relativity.kepler.client" Version="2.9.2" />
		<PackageVersion Include="relativity.oauth2client" Version="1.5.2" />
		<PackageVersion Include="relativity.oauth2client.implicit" Version="1.5.2" />
		<PackageVersion Include="relativity.oauth2client.interfaces" Version="1.5.2" />
